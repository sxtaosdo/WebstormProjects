    <script>
        function disableMove(event) {
            if (event && event.targetTouches && event.targetTouches.length > 1) {
                event.preventDefault();
            }
        }
        function disableTouchStart(event) {
            event.preventDefault();
        }

        function enableBrowserZoomH() {
            document.body.addEventListener( 'touchmove', disableMove, false );
            document.documentElement.addEventListener('touchstart', disableTouchStart, false);
        }

        function disableBrowserZoomH() {
            document.body.removeEventListener( 'touchmove', disableMove );
            document.documentElement.removeEventListener( 'touchstart', disableTouchStart );
        }

        enableBrowserZoomH();
    </script>
